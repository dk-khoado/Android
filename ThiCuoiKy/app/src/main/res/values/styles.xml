<resources>

    <!-- Base application theme. -->
    <style name="AppThem" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->

        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppThem..no" parent="Theme.AppCompat.Light.NoActionBar">

    </style>

    <style name="AppThem.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppThem.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppThem.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>
